{
    "name": "oh86/laravel-captcha",
    "description": "captcha for app with laravel framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oh86\\Captcha\\": "src/"
        }
    },
    "authors": [
        {
            "name": "chengj",
            "email": "chen.gj@qq.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "mews/captcha": "^3.3"
    },
    "require-dev": {
        "laravel/framework": "^7.0||^8.0||^9.0||^10.0||^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oh86\\Captcha\\CaptchaServiceProvider"
            ],
            "dont-discover": [
                "mews/captcha"
            ]
        }
    }
}